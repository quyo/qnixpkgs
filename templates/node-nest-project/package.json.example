{
  "name": "node-nest-project",
  "version": "1.0.0",
  "description": "",
  "main": "./src/backend/app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lint-eslint": "eslint . --fix --cache --cache-strategy content",
    "lint-stylelint": "stylelint 'src/frontend/**/*.{css,scss,sass,less}' --fix --cache --cache-strategy content",
    "lint": "run-p lint-eslint lint-stylelint",
    "prettify_backend_onchange": "fd --changed-within 1m . config/ src/backend/ --exec-batch prettier --write --cache --cache-strategy content --ignore-unknown --no-error-on-unmatched-pattern",
    "prettify_frontend_onchange": "fd --changed-within 1m . src/frontend/ --exec-batch prettier --write --cache --cache-strategy content --ignore-unknown --no-error-on-unmatched-pattern",
    "prettify": "prettier --write --cache --cache-strategy content --no-error-on-unmatched-pattern .",
    "clean": "gulp clean",
    "build_backend": "gulp backend",
    "build_frontend_assets": "gulp frontend-assets",
    "build_frontend_views": "gulp frontend-views",
    "build_frontend_compile:dev": "webpack --mode development",
    "build_frontend_compile:prod": "webpack --mode production",
    "build_frontend:dev": "run-p build_frontend_assets build_frontend_views build_frontend_compile:dev",
    "build_frontend:prod": "run-p build_frontend_assets build_frontend_views build_frontend_compile:prod",
    "build:dev": "npm run clean && run-p build_backend build_frontend:dev",
    "build:prod": "npm run clean && run-p build_backend build_frontend:prod",
    "serve:dev": "NODE_ENV=development ts-node --transpile-only --experimental-specifier-resolution=node ./src/backend/app.ts",
    "serve:prod": "NODE_ENV=production node --experimental-specifier-resolution=node ./dist/backend/app.js",
    "build_serve:dev": "run-s build:dev serve:dev",
    "build_serve:prod": "run-s build:prod serve:prod",
    "watch_backend": "nodemon --watch config --watch src/backend -e ts,tsx,cts,mts,js,jsx,cjs,mjs,json,properties --exec run-s prettify_backend_onchange serve:dev",
    "watch_frontend_assets": "nodemon --watch src/frontend/public -e '*' --ignore '**/*.{ts,tsx,cts,mts,js,jsx,cjs,mjs,css,scss,sass,less,html,htm}' --exec npm run build_frontend_assets",
    "watch_frontend_compile": "nodemon --watch src/frontend/public -e ts,tsx,cts,mts,js,jsx,cjs,mjs,css,scss,sass,less,html,htm --exec run-s prettify_frontend_onchange build_frontend_compile:dev",
    "watch_frontend_views": "nodemon --watch src/frontend/views -e '*' --exec run-s prettify_frontend_onchange build_frontend_views",
    "watch": "npm run clean && run-p watch_backend watch_frontend_assets watch_frontend_views watch_frontend_compile",
    "dev": "npm run watch",
    "prod": "npm run build_serve:prod"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/config": "^3.3.0",
    "@types/ejs": "^3.1.1",
    "@types/express": "^4.17.14",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/gulp": "^4.0.9",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/node": "^18.11.9",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "del": "^7.0.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "less-loader": "^11.1.0",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.14.1",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-less": "^1.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-less": "^1.0.6",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "@nestjs/common": "^9.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/platform-express": "^9.2.0",
    "@nestjs/testing": "^9.2.0",
    "config": "^3.3.8",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-ejs-layouts": "^2.5.1",
    "properties": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.7"
  },
  "type": "module"
}
